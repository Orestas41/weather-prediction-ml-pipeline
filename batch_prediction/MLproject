name: batch_prediction
conda_env: conda.yml

entry_points:
  main:
    parameters:
      reg_model:
        description: An MLflow serialized model
        type: string

      class_model:
        description: An MLflow serialized model
        type: string

      full_dataset:
        description: Full training dataset with the latest data
        type: string

    command: "python run.py --reg_model {reg_model} --class_model {class_model} --full_dataset {full_dataset}"