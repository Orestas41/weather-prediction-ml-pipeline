name: batch_prediction
conda_env: conda.yml

entry_points:
  main:
    parameters:
      mlflow_model:
        description: An MLflow serialized model
        type: string

      full_dataset:
        description: Full training dataset with the latest data
        type: string

    command: "python run.py --mlflow_model {mlflow_model} --full_dataset {full_dataset}"